<?xml version="1.0" encoding="UTF-8"?>

<faces-config
    xmlns="http://java.sun.com/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd"
    version="2.0">
    
    <application>
		<resource-bundle>
			<base-name>net.entrofi.tutorials.jee.ecollector.messages.messages</base-name>
			<var>messages</var>
		</resource-bundle>
		<resource-bundle>
			<base-name>net.entrofi.tutorials.jee.ecollector.labels.labels</base-name>
			<var>labels</var>
		</resource-bundle>
		<resource-bundle>
			<base-name>net.entrofi.tutorials.jee.ecollector.staticinfo.info</base-name>
			<var>info</var>
		</resource-bundle>
	</application>

</faces-config>
